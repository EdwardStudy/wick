#!/bin/bash
#
# Shows the help message

wick-on-load wick-add-command help "This help screen"

# Show the help messages saved with wick-add-command
wick-help() {
    echo "Specify a command:"
    echo ""

    if [[ "${#WICK_COMMAND_HELP[@]}" -eq 0 ]]; then
        echo "No commands defined"
        return
    fi

    for F in "${WICK_COMMAND_HELP[@]}"; do
        echo "$F"
    done
}
